{
  "extends": "tslint:recommended",
  "rules": {
    "ordered-imports": false,
    "trailing-comma": false,
    "interface-over-type-literal": false,
    "arrow-parens": false,
    "member-access": false,
    "no-namespace": false,
    "no-console": false,
    "max-line-length": {
      "options": [120]
    },
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false
  },
  "jsRules": {
    "max-line-length": {
      "options": [120]
    }
  }
}
